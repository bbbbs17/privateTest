<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/consumption#"
     xml:base="http://www.semanticweb.org/consumption"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://www.semanticweb.org/consumption"/>

    <!-- 클래스 정의 -->
    <owl:Class rdf:about="#Consumption"/>
    <owl:Class rdf:about="#HabitualConsumption">
        <rdfs:subClassOf rdf:resource="#Consumption"/>
    </owl:Class>
    <owl:Class rdf:about="#Item"/>
    <owl:Class rdf:about="#TimeSlot"/>
    <owl:Class rdf:about="#Pattern"/>

    <!-- 속성 정의 -->
    <owl:ObjectProperty rdf:about="#hasItem"/>
    <owl:ObjectProperty rdf:about="#hasTimeSlot"/>
    <owl:DatatypeProperty rdf:about="#hasPattern"/>

</rdf:RDF>
